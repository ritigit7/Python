    "import PySimpleGUI as sg\n",
    "\n",
    "layout=[[sg.Text(text=\"Hello world\",justification='center')],]\n",
    "window=sg.Window(\"Trial\",layout,size=(200,100))\n",
    "while True:\n",
    "    event,value=window.read()\n",
    "    print(event,value)\n",
    "    if event in (None,'Exit'):\n",
    "        break\n",
    "window.close()"
   ]
